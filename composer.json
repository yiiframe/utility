{
    "name": "yiiframe/utility",
    "type": "library",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/yiiframe/utility",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiiframe/utility",
        "issues": "https://github.com/yiiframe/utility/issues",
        "wiki": "https://github.com/yiiframe/utility"
    },
    "require": {
        "php": ">=7.2.0",
        "ext-pdo": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "ext-json": "*",
        "yiisoft/yii2": "~2.0.20",
        "yiisoft/yii2-httpclient": "~2.0.11",
        "imangazaliev/didom": "~1.14.1",
        "sabberworm/php-css-parser": "~8.3.0",
        "phpoffice/phpspreadsheet": "~1.8.1 || ~1.10.1",
        "guzzlehttp/guzzle": "~6.3.3 || ~6.5.3"
    },
    "require-dev": {
        "yiisoft/yii2-faker": "~2.0.4",
        "phpunit/phpunit": "~8.2.0",
        "nikic/php-parser": "~4.2.2",
        "jdorn/sql-formatter": "dev-master",
        "roave/security-advisories": "dev-master"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Casoa\\Yii\\Utility\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Casoa\\Yii\\Utility\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "composer dump-autoload && php vendor/phpunit/phpunit/phpunit"
    }
}
